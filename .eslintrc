{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "quotes": "off",
        "require-jsdoc": "off",
        "max-len": "off",
        "no-debugger": "off"
    },
    "extends": [
        "eslint:recommended", 
        "google"
    ]
}